openness_questions
    use openness($avg)
    when
        questions.openness_1($ans1)
        questions.openness_2($ans2)
        questions.openness_3($ans3)
        questions.openness_4($ans4)

        $tot = $ans1 + $ans2 + $ans3 + $ans4
        $avg = $tot / 4

openness_test
    use what_is_the_personality(lively)
    when
        questions.gender($gender)
        questions.age($age)
        openness($avg_openness)
        check $avg_openness >= 5.0